{
  "Serilog": {
    "MinimumLevel": "Information"
  },
  "Security": {
    "RequireMfa": true,
    "RequirePasskeyEnrollment": true,
    "AccountLockoutMaxFailedAttempts": 5
  },
  "Fido2": {
    "RelyingPartyId": "localhost",
    "RelyingPartyName": "GovConMoney",
    "Origins": [
      "https://localhost:7289"
    ]
  },
  "SubmissionRules": {
    "DailyEntryGraceDays": 1,
    "MaxHoursPerDay": 24
  },
  "ConnectionStrings": {
    "Primary": "Server=(localdb)\\MSSQLLocalDB;Database=GovConMoney;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True;",
    "Identity": "Server=(localdb)\\MSSQLLocalDB;Database=GovConMoneyIdentity;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True;"
  },
  "ExternalServices": {
    "OpenAI": {
      "Endpoint": "https://api.openai.com",
      "ApiKey": "${OPENAI_API_KEY}"
    },
    "SamGov": {
      "Endpoint": "https://api.sam.gov",
      "ApiKey": "${SAM_GOV_API_KEY}"
    }
  }
}
